doctype html
html(lang="en")
  head
    meta(charset="utf-8")
    link(rel="apple-touch-icon" sizes="76x76" href="/paper-kit/assets/img//apple-icon.png")
    link(rel="icon" type="image/png" href="/paper-kit/assets/img//favicon.png")
    meta(http-equiv="X-UA-Compatible" content="IE=edge,chrome=1")
    title
      | Paper Kit by Creative Tim
    meta(content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no" name="viewport")
    // Fonts and icons
    link(href="https://fonts.googleapis.com/css?family=Montserrat:400,700,200" rel="stylesheet")
    link(href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet")
    // CSS Files
    link(href="/paper-kit/assets/css/bootstrap.min.css" rel="stylesheet")
    link(href="/paper-kit/assets/css/paper-kit.css?v=2.2.0" rel="stylesheet")
    link(href="/paper-kit/assets/css/all.min.css" rel="stylesheet")
    // CSS Just for demo purpose, don't include it in your project
    link(href="/paper-kit/assets/demo/demo.css" rel="stylesheet")
  body.index-page.sidebar-collapse
    // Navbar
    include nav
    // End Navbar
    include header
    .main
      include about
      include representation
      include inside
      include outside
      include brandingStory
      include partnerShip
      include request
      include way

      footer.footer.footer-black.footer-white
        .container
          .row
            nav.footer-nav
              ul
                li
                  a(href="#about") 회사소개
                li
                  a(href="#representation") 대표소개
                li
                  a(href="#way") 오시는 길
            .credits.ml-auto
              span.copyright
                | © 퍼스널브랜드 유니온, made with
                i.fa.fa-heart.heart
                |  by Ho.IT
      // Core JS Files
      script(src="/paper-kit/assets/js/core/jquery.min.js" type="text/javascript")
      script(src="/paper-kit/assets/js/core/popper.min.js" type="text/javascript")
      script(src="/paper-kit/assets/js/core/bootstrap.min.js" type="text/javascript")
      // Plugin for Switches, full documentation here: http://www.jque.re/plugins/version3/bootstrap.switch/
      script(src="/paper-kit/assets/js/plugins/bootstrap-switch.js")
      // Plugin for the Sliders, full documentation here: http://refreshless.com/nouislider/
      script(src="/paper-kit/assets/js/plugins/nouislider.min.js" type="text/javascript")
      // Plugin for the DatePicker, full documentation here: https://github.com/uxsolutions/bootstrap-datepicker
      script(src="/paper-kit/assets/js/plugins/moment.min.js")
      script(src="/paper-kit/assets/js/plugins/bootstrap-datepicker.js" type="text/javascript")
      // Control Center for Paper Kit: parallax effects, scripts for the example pages etc
      script(src="/paper-kit/assets/js/paper-kit.js?v=2.2.0" type="text/javascript")
      script(type="text/javascript" src="//dapi.kakao.com/v2/maps/sdk.js?appkey=a808278c2a3cff6f949c8b7a63e44534")
      script.
        $(document).ready(function() {
          if ($("#datetimepicker").length != 0) {
            $('#datetimepicker').datetimepicker({
              icons: {
                time: "fa fa-clock-o",
                date: "fa fa-calendar",
                up: "fa fa-chevron-up",
                down: "fa fa-chevron-down",
                previous: 'fa fa-chevron-left',
                next: 'fa fa-chevron-right',
                today: 'fa fa-screenshot',
                clear: 'fa fa-trash',
                close: 'fa fa-remove'
              }
            });
          }
          function scrollToDownload() {
            if ($('.section-download').length != 0) {
              $("html, body").animate({
                scrollTop: $('.section-download').offset().top
              }, 1000);
            }
          }
          // kakao map
          var container = document.getElementById('map'); //지도를 담을 영역의 DOM 레퍼런스
          var options = { //지도를 생성할 때 필요한 기본 옵션
            center: new kakao.maps.LatLng(33.450701, 126.570667), //지도의 중심좌표.
            level: 3 //지도의 레벨(확대, 축소 정도)
          };
          var map = new kakao.maps.Map(container, options); //지도 생성 및 객체 리턴

        });
