doctype html
html(lang="en")
	head
		meta(charset="utf-8")
		link(rel="apple-touch-icon" sizes="76x76" href="/nm/paper-dashboard-2/assets/img/apple-icon.png")
		link(rel="icon" type="image/png" href="/nm/paper-dashboard-2/assets/img/favicon.png")
		meta(http-equiv="X-UA-Compatible" content="IE=edge,chrome=1")
		title
			| Paper Dashboard 2 by Creative Tim
		meta(content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no" name="viewport")
		// Fonts and icons
		link(href="https://fonts.googleapis.com/css?family=Montserrat:400,700,200" rel="stylesheet")
		link(href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet")
		// CSS Files
		link(href="/nm/paper-dashboard-2/assets/css/bootstrap.min.css" rel="stylesheet")
		link(href="/nm/paper-dashboard-2/assets/css/paper-dashboard.css?v=2.0.0" rel="stylesheet")
		// CSS Just for demo purpose, don't include it in your project
		link(href="/nm/paper-dashboard-2/assets/demo/demo.css" rel="stylesheet")
	body
		.wrapper
			.sidebar(data-color="white" data-active-color="danger")
				//
					Tip 1: You can change the color of the sidebar using: data-color="blue | green | orange | red | yellow"
				.logo
					a.simple-text.logo-mini(href="http://www.creative-tim.com")
						.logo-image-small
							img(src="/nm/paper-dashboard-2/assets/img/logo-small.png")
					a.simple-text.logo-normal(href="http://www.creative-tim.com")
						| Creative Tim
						//
							<div class="logo-image-big">
							<img src="/nm/paper-dashboard-2/assets/img/logo-big.png">
							</div>
				.sidebar-wrapper
					ul.nav
						li.active
							a(href="./dashboard.html")
								i.nc-icon.nc-bank
								p Dashboard
						li
							a(href="./icons.html")
								i.nc-icon.nc-diamond
								p Icons
						li
							a(href="./map.html")
								i.nc-icon.nc-pin-3
								p Maps
						li
							a(href="./notifications.html")
								i.nc-icon.nc-bell-55
								p Notifications
						li
							a(href="./user.html")
								i.nc-icon.nc-single-02
								p User Profile
						li
							a(href="./tables.html")
								i.nc-icon.nc-tile-56
								p Table List
						li
							a(href="./typography.html")
								i.nc-icon.nc-caps-small
								p Typography
						li.active-pro
							a(href="./upgrade.html")
								i.nc-icon.nc-spaceship
								p Upgrade to PRO
			.main-panel
				// Navbar
				nav.navbar.navbar-expand-lg.navbar-absolute.fixed-top.navbar-transparent
					.container-fluid
						.navbar-wrapper
							.navbar-toggle
								button.navbar-toggler(type="button")
									span.navbar-toggler-bar.bar1
									span.navbar-toggler-bar.bar2
									span.navbar-toggler-bar.bar3
							a.navbar-brand(href="#pablo") Paper Dashboard 2
						button.navbar-toggler(type="button" data-toggle="collapse" data-target="#navigation" aria-controls="navigation-index" aria-expanded="false" aria-label="Toggle navigation")
							span.navbar-toggler-bar.navbar-kebab
							span.navbar-toggler-bar.navbar-kebab
							span.navbar-toggler-bar.navbar-kebab
						#navigation.collapse.navbar-collapse.justify-content-end
							form
								.input-group.no-border
									input.form-control(type="text" value="" placeholder="Search...")
									.input-group-append
										.input-group-text
											i.nc-icon.nc-zoom-split
							ul.navbar-nav
								li.nav-item
									a.nav-link.btn-magnify(href="#pablo")
										i.nc-icon.nc-layout-11
										p
											span.d-lg-none.d-md-block Stats
								li.nav-item.btn-rotate.dropdown
									a#navbarDropdownMenuLink.nav-link.dropdown-toggle(href="http://example.com" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false")
										i.nc-icon.nc-bell-55
										p
											span.d-lg-none.d-md-block Some Actions
									.dropdown-menu.dropdown-menu-right(aria-labelledby="navbarDropdownMenuLink")
										a.dropdown-item(href="#") Action
										a.dropdown-item(href="#") Another action
										a.dropdown-item(href="#") Something else here
								li.nav-item
									a.nav-link.btn-rotate(href="#pablo")
										i.nc-icon.nc-settings-gear-65
										p
											span.d-lg-none.d-md-block Account
				// End Navbar
				//
					<div class="panel-header panel-header-lg">
					<canvas id="bigDashboardChart"></canvas>
					</div>
				.content
					.row
						.col-lg-3.col-md-6.col-sm-6
							.card.card-stats
								.card-body
									.row
										.col-5.col-md-4
											.icon-big.text-center.icon-warning
												i.nc-icon.nc-globe.text-warning
										.col-7.col-md-8
											.numbers
												p.card-category Capacity
												p.card-title
													| 150GB
												p
								.card-footer
									hr
									.stats
										i.fa.fa-refresh
										|  Update Now
						.col-lg-3.col-md-6.col-sm-6
							.card.card-stats
								.card-body
									.row
										.col-5.col-md-4
											.icon-big.text-center.icon-warning
												i.nc-icon.nc-money-coins.text-success
										.col-7.col-md-8
											.numbers
												p.card-category Revenue
												p.card-title
													| $ 1,345
												p
								.card-footer
									hr
									.stats
										i.fa.fa-calendar-o
										|  Last day
						.col-lg-3.col-md-6.col-sm-6
							.card.card-stats
								.card-body
									.row
										.col-5.col-md-4
											.icon-big.text-center.icon-warning
												i.nc-icon.nc-vector.text-danger
										.col-7.col-md-8
											.numbers
												p.card-category Errors
												p.card-title
													| 23
												p
								.card-footer
									hr
									.stats
										i.fa.fa-clock-o
										|  In the last hour
						.col-lg-3.col-md-6.col-sm-6
							.card.card-stats
								.card-body
									.row
										.col-5.col-md-4
											.icon-big.text-center.icon-warning
												i.nc-icon.nc-favourite-28.text-primary
										.col-7.col-md-8
											.numbers
												p.card-category Followers
												p.card-title
													| +45K
												p
								.card-footer
									hr
									.stats
										i.fa.fa-refresh
										|  Update now
					.row
						.col-md-12
							.card
								.card-header
									h5.card-title Users Behavior
									p.card-category 24 Hours performance
								.card-body
									canvas#chartHours(width="400" height="100")
								.card-footer
									hr
									.stats
										i.fa.fa-history
										|  Updated 3 minutes ago
					.row
						.col-md-4
							.card
								.card-header
									h5.card-title Email Statistics
									p.card-category Last Campaign Performance
								.card-body
									canvas#chartEmail
								.card-footer
									.legend
										i.fa.fa-circle.text-primary
										|  Opened
										i.fa.fa-circle.text-warning
										|  Read
										i.fa.fa-circle.text-danger
										|  Deleted
										i.fa.fa-circle.text-gray
										|  Unopened
									hr
									.stats
										i.fa.fa-calendar
										|  Number of emails sent
						.col-md-8
							.card.card-chart
								.card-header
									h5.card-title NASDAQ: AAPL
									p.card-category Line Chart with Points
								.card-body
									canvas#speedChart(width="400" height="100")
								.card-footer
									.chart-legend
										i.fa.fa-circle.text-info
										|  Tesla Model S
										i.fa.fa-circle.text-warning
										|  BMW 5 Series
									hr
									.card-stats
										i.fa.fa-check
										|  Data information certified
				footer.footer.footer-black.footer-white
					.container-fluid
						.row
							nav.footer-nav
								ul
									li
										a(href="https://www.creative-tim.com" target="_blank") Creative Tim
									li
										a(href="http://blog.creative-tim.com/" target="_blank") Blog
									li
										a(href="https://www.creative-tim.com/license" target="_blank") Licenses
							.credits.ml-auto
								span.copyright
									| Â© PB UNION made with 
									i.fa.fa-heart.heart
									|  by Ho.IT
